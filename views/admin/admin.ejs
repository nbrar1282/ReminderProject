<!DOCTYPE html>
<html lang="en">
<head>
<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<title>Admin Dashboard</title>
<!-- Include any additional CSS or JS here -->
</head>
<body>
  <h1>Dashboard</h1>
  <p>Welcome, <%= adminName %>!</p> <!-- Dynamically set the admin's name -->
  <div id="sessions">
    <h2>Current Active Sessions:</h2>
    <% if (sessions && sessions.length) { %>
      <ul>
        <% sessions.forEach(function(session) { %>
          <li class="session-item">
            SessionID: <%= session.id %> - UserID: <%= session.userId %>
            <!-- Replace 'data-session-id' with the appropriate attribute for your session IDs -->
            <button class="revoke-session-button" data-session-id="<%= session.id %>">Revoke Session</button>
          </li>
        <% }); %>
      </ul>
    <% } else { %>
      <p>No active sessions.</p>
    <% } %>
  </div>

  <script>
    // Your JavaScript will go here, you can either include it inline or link to a separate .js file
    // The client-side JavaScript provided in the previous example would go here.
  </script>
</body>
</html>
